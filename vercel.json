{
    "projects": [
      {
        "src": "frontend",
        "use": "@vercel/static-build"
      },
      {
        "src": "backend",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/src/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/frontend/$1"
      }
    ]
  }
  